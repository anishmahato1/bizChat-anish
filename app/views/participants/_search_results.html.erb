    <%=turbo_frame_tag user do %>
    <div class="search-results-each">
        <%= image_tag(user.avatar.variant(:small), alt: "User Avatar", class: "search-results-each-avatar") if user.avatar.attached? %>
        <h3><%= user.name %></h3>
        <h4><%= user.email %></h4>
        <%= button_to channel_add_participants_path(channel,user), class: "add-participants-btn", method: :post do%><i class="fa-solid fa-plus"></i><% end %>
    </div>
    <% end %>

