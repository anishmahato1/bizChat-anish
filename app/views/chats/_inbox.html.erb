<%= link_to chat_path(inbox), data: {turbo_frame: "central-and-right-panels"} do %>
<div class="inboxes-each" id= <%=dom_id(inbox)%> >
    <%= image_tag(inbox.users[0].avatar.variant(:small), alt: "User Avatar", class: "inboxes-each-avatar") if inbox.users[0].avatar.attached? %>
    <h3><%=inbox.users[0].name%></h3><br>
    <h4><%=inbox.last_message.present? ? inbox.last_message.content : ""%></h4>
</div>
<% end %>